numbers = input("Enter numbers separated by spaces: ")

numbers = [int(num) for num in numbers.split()]

even_count = 0
odd_count = 0

for num in numbers:
    if num % 2 == 0:
        even_count += 1
    else:
        odd_count += 1

print(f"\nThere are {even_count} even numbers and {odd_count} odd numbers.")
